    "eyear": "",                                           //结束年份（此处无需给定，数据列表中已给出）
    "suffix": "",                                          //数据名称时间后缀（此处无需给定，数据列表中已给出）
    "prefix": "",                                          //数据名称时间前缀（此处无需给定，数据列表中已给出）
    "root_dir": "./data/simulation/debug/stn"              //数据存放路径
  }
